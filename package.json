{
  "name": "electron-webview-todo",
  "version": "0.0.1",
  "description": "Desktop app using Electron which bundles a React App written in ES6 + React + Redux + Webpack + CSS Modules",
  "main": "src/main-process/main.js",
  "scripts": {
    "start": "cross-env DEBUG=* electron . --enable-logging",
    "lint": "eslint src",
    "package": "electron-packager . --out=dist --asar --overwrite --all",
    "package:win32": "electron-packager . --out=dist --asar --overwrite --platform=win32 --arch=ia32",
    "package:win64": "electron-packager . --out=dist --asar --overwrite --platform=win32 --arch=x64",
    "package:osx": "electron-packager . --out=dist --asar --overwrite --platform=mas --arch=x64"
  },
  "devDependencies": {
    "cross-env": "^3.1.4",
    "electron": "^1.4.13",
    "electron-packager": "^8.4.0"
  },
  "repository": "https://github.com/deepak/electron-webview-todo",
  "keywords": [
    "ES6",
    "react",
    "redux",
    "electron"
  ],
  "private": true,
  "license": "MIT",
  "author": "Deepak Kannan"
}
